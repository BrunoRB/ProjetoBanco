<?php


class PostgresConnect{
	protected $host;
	protected $port;
	protected $databaseName;
	protected $user;
	protected $password;

	public function __construct($host, $databaseName, $user, $password, $port = 5432) {
		$this->host = $host;
		$this->port = $port;
		$this->databaseName = $databaseName;
		$this->user = $user;
		$this->password = $password;

	}
}